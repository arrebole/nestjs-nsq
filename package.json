{
  "name": "nestjs-nsq",
  "version": "0.0.2",
  "description": "nestjs nsq module",
  "author": "arrebole",
  "private": false,
  "license": "UNLICENSED",
  "main": "./dist/index.js",
  "files": [
    "dist",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/arrebole/nestjs-nsq"
  },
  "scripts": {
    "test": "jest",
    "build": "tsc --project tsconfig.build.json",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\""
  },
  "devDependencies": {
    "@nestjs/common": "^8.2.3",
    "@nestjs/testing": "^8.2.3",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.12",
    "@types/nsqjs": "^0.12.1",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.4.3",
    "nsqjs": "^0.13.0",
    "prettier": "^2.5.1",
    "reflect-metadata": "^0.1.13",
    "supertest": "^6.1.6",
    "ts-jest": "^27.1.1",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "test",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
